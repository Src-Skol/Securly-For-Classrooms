{
  "manifest_version": 2,
  "name": "Securly Classroom",
  "short_name": "Securly",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "1.20.03",
  "update_url": "https://deviceconsole.securly.com/dist/chrome/n.xml",
  "key": "",
  "icons": {
    "16": "logo16mono.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "background": {
    "scripts": ["c.js","f.js","lo.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "logo16mono.png",
      "48": "logo48.png",
      "128": "logo128.png"
    },
    "default_title": "Securly Classroom"
  },
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "activeTab",
    "alarms",
    "declarativeNetRequest",
    "enterprise.deviceAttributes",
    "gcm",
    "geolocation",
    "identity",
    "identity.email",
    "idle",
    "nativeMessaging",
    "notifications",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' object-src 'self'",
  "externally_connectable": {
    "ids": ["*"]
  },
  "web_accessible_resources": [
    "*",
    "blocking/*",
    "fonts/*"
  ]
}
